{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "service-name",
    "labels": {
      "k8s-app": "app-name"
    }
  },
  "spec": {
    "ports": [
      {
        "name": "service-tcp-name",
        "protocol": "TCP",
        "port": 80,
        "targetPort": 80
      }
    ],
    "type": "LoadBalancer",
    "sessionAffinity": "None",
    "externalTrafficPolicy": "Cluster"
  }
}
